{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "dist/": true,
        "**/*.js.map": {
            "when": "$(basename)"
        },
        "**/*.js": {
            "when": "$(basename).ts"
        }
    }
}